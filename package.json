{
  "name": "tailwind-rn-gust",
  "version": "0.0.1",
  "description": "className support for tailwind in React Native",
  "main": "dist/index.js",
  "repository": "https://github.com/jakobo/tailwind-rn-gust",
  "author": "Jakob Heuser <jakob@codedrift.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
